generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model launches {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  v            Int      @map("__v")
  customers    String[]
  flightNumber Int
  launchDate   DateTime @db.Date
  mission      String
  rocket       String
  success      Boolean?
  upcoming     Boolean
}

model planets {
  id         String @id @default(auto()) @map("_id") @db.ObjectId
  v          Int    @map("__v")
  keplerName String
}
